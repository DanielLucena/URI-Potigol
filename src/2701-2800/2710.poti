n = leia_inteiro
mat = Matriz.mutável(500, 500, 0)
para i de 1 até n faça
  linha = leia_textos(" ")
  se linha[1] == "U" então
    para x de linha[2].inteiro até linha[4].inteiro,
         y de linha[3].inteiro até linha[5].inteiro faça
           mat[x][y] := mat[x][y] + linha[6].inteiro
    fim
  senão
    escreva mat[linha[2].inteiro][linha[3].inteiro]
  fim
fim
