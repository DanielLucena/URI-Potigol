entrada = leia_reais(" ")
x, y = entrada[1], entrada[2]
escolha Tupla(x,y)
  caso Tupla(0.0, 0.0)        => escreva "Origem"
  caso Tupla(_, 0.0)          => escreva "Eixo X"
  caso Tupla(0.0, _)          => escreva "Eixo Y"
  caso _ se x > 0.0 e y > 0.0 => escreva "Q1"
  caso _ se y > 0.0           => escreva "Q2"
  caso _ se x < 0.0           => escreva "Q3"
  caso _                      => escreva "Q4"
fim
